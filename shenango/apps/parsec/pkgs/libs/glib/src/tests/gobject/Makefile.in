clean-noinstLTLIBRARIES:
	-test -z "$(noinst_LTLIBRARIES)" || rm -f $(noinst_LTLIBRARIES)
	@list='$(noinst_LTLIBRARIES)'; for p in $$list; do \
	  dir="`echo $$p | sed -e 's|/[^/]*$$||'`"; \
	  test "$$dir" != "$$p" || dir=.; \
	  echo "rm -f \"$${dir}/so_locations\""; \
	  rm -f "$${dir}/so_locations"; \
	done
libtestgobject.la: $(libtestgobject_la_OBJECTS) $(libtestgobject_la_DEPENDENCIES) 
	$(AM_V_CCLD)$(LINK)  $(libtestgobject_la_OBJECTS) $(libtestgobject_la_LIBADD) $(LIBS)

