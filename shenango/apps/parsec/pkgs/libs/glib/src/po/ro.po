#, c-format
msgid "%.1f KB"
msgstr "%.1f KB"

#: glib/gfileutils.c:1787
#, c-format
msgid "%.1f MB"
msgstr "%.1f MB"

#: glib/gfileutils.c:1792
#, c-format
msgid "%.1f GB"
msgstr "%.1f GB"

#: glib/gfileutils.c:1797
#, c-format
msgid "%.1f TB"
msgstr "%.1f TB"

